#SCMBAG aliases

alias scm='/usr/local/share/scmbag/scmbag'
alias gs='scm -s'
alias ga='scm -a'
alias gd='scm -d'
alias gps='git push'
alias gdc='git diff --cached'
alias gc='git commit'
alias gcm='scm -c'
alias gcl='git clone'
alias gcma='scm -m'
alias gl='git log --graph'
alias gg='git log --graph --decorate --oneline'
alias grs='scm -r'
alias gb='git branch'
alias gco='scm -o'
alias grm='scm -x'
alias gi='scm --ignore'
